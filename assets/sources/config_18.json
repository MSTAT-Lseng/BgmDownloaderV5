{
  "search_config": {
    "url": "{host}/index.php/vod/search.html?wd={q}",
    "details_format": "{host}{result}",
    "item_regex": "<dl class=\"fed-deta-info[\\s\\S]*?<a[^>]+href=\"([^\"]+)\"[^>]+data-original=\"([^\"]+)\"[\\s\\S]*?<h1[^>]*>[\\s\\S]*?<a[^>]*>(.*?)</a>",
    "fields": {
      "url": {
        "match_index": 1,
        "description": "详情页跳转链接"
      },
      "image": {
        "match_index": 2,
        "description": "作品封面图片地址"
      },
      "title": {
        "match_index": 3,
        "description": "作品标题名称"
      }
    }
  }
}