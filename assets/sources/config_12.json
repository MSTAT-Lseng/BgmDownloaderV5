{
  "search_config": {
    "url": "{host}/search/-------------/?wd={q}",
    "details_format": "{host}{result}",
    "item_regex": "<div\\s+class=\"vod-detail\\s+style-detail\\s+cor4\\s+search-list\"[\\s\\S]*?<img[^>]*\\bdata-src=\"([^\"]+)\"[^>]*>[\\s\\S]*?<a[^>]*href=\"([^\"]+)\"[^>]*>[\\s\\S]*?<h3[^>]*class=\"slide-info-title[^\"]*\"[^>]*>\\s*([^<]+?)\\s*<\\/h3>[\\s\\S]*?<\\/div>\\s*<\\/div>\\s*<\\/div>\\s*<\\/div>",
    "fields": {
      "url": {
        "match_index": 2,
        "description": "详情页跳转链接"
      },
      "image": {
        "match_index": 1,
        "description": "作品封面图片地址"
      },
      "title": {
        "match_index": 3,
        "description": "作品标题名称"
      }
    }
  }
}