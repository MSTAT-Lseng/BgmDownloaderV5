{
  "search_config": {
    "url": "{host}/feng-s.html?wd={q}&submit=",
    "details_format": "{host}{result}",
    "item_regex": "<li class=\"hl-list-item hl-col-xs-12\">.*?<a class=\"hl-item-thumb hl-lazy\" href=\"(.*?)\" title=\"(.*?)\" data-original=\"(.*?)\"",
    "fields": {
      "url": {
        "match_index": 1,
        "description": "详情页跳转链接"
      },
      "title": {
        "match_index": 2,
        "description": "作品标题名称"
      },
      "image": {
        "match_index": 3,
        "description": "作品封面图片地址"
      }
    }
  }
}