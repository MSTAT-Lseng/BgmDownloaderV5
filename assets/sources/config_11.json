{
  "search_config": {
    "url": "{host}/s----------.html?wd={q}",
    "details_format": "{host}{result}",
    "item_regex": "data-bg=\"([^\"]+)\".*?href=\"([^\"]+)\".*?title=\"([^\"]+)\"",
    "fields": {
      "url": {
        "match_index": 2,
        "description": "详情页跳转链接"
      },
      "image": {
        "match_index": 1,
        "description": "作品封面图片地址"
      },
      "title": {
        "match_index": 3,
        "description": "作品标题名称"
      }
    }
  }
}