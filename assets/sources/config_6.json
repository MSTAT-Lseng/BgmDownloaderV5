{
  "search_config": {
    "url": "{host}/search_-------------.html?wd={q}",
    "details_format": "{host}{result}",
    "item_regex": "<div class=\"module-card-item module-item\">.*?<a href=\"(.*?)\" class=\"module-card-item-poster\">.*?data-original=\"(.*?)\" alt=\"(.*?)\"",
    "fields": {
      "url": {
        "match_index": 1,
        "description": "详情页跳转链接"
      },
      "image": {
        "match_index": 2,
        "description": "作品封面图片地址"
      },
      "title": {
        "match_index": 3,
        "description": "作品标题名称"
      }
    }
  }
}