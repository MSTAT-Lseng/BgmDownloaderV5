{
  "search_config": {
    "url": "{host}/index.php/vod/search.html?wd={q}",
    "details_format": "{host}{result}",
    "item_regex": "<li[^>]*class=\"[^\"]*col-[^\"]*\">[\\s\\S]*?<div class=\"pic\"><a href=\"([^\"]+)\" title=\"([^\"]+)\">[\\s\\S]*?data-original=\"([^\"]+)\"",
    "fields": {
      "url": {
        "match_index": 1,
        "description": "详情页跳转链接"
      },
      "title": {
        "match_index": 2,
        "description": "作品标题名称"
      },
      "image": {
        "match_index": 3,
        "description": "作品封面图片地址"
      }
    }
  }
}